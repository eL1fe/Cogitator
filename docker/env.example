# Cogitator Environment Configuration
# Copy this file to .env in the project root and customize as needed

# ============================================================================
# Database
# ============================================================================
DATABASE_URL=postgresql://cogitator:cogitator_dev@localhost:5432/cogitator
POSTGRES_HOST=localhost
POSTGRES_PORT=5432
POSTGRES_USER=cogitator
POSTGRES_PASSWORD=cogitator_dev
POSTGRES_DB=cogitator

# ============================================================================
# Redis
# ============================================================================
REDIS_URL=redis://localhost:6379
REDIS_HOST=localhost
REDIS_PORT=6379

# ============================================================================
# Ollama (Local LLMs)
# ============================================================================
OLLAMA_URL=http://localhost:11434

# ============================================================================
# Cloud LLM Providers (optional)
# ============================================================================
# OPENAI_API_KEY=sk-...
# ANTHROPIC_API_KEY=sk-ant-...
# GOOGLE_API_KEY=AIza...

# ============================================================================
# Memory & Embeddings
# ============================================================================
# Embedding model for semantic memory (default: nomic-embed-text-v2-moe via Ollama)
EMBEDDING_PROVIDER=ollama
EMBEDDING_MODEL=nomic-embed-text-v2-moe

# ============================================================================
# Sandbox (Code Execution)
# ============================================================================
SANDBOX_DEFAULT_TYPE=native
SANDBOX_DEFAULT_TIMEOUT=30000
SANDBOX_MAX_CONTAINERS=5

# ============================================================================
# Dashboard
# ============================================================================
NEXT_PUBLIC_APP_URL=http://localhost:3000

