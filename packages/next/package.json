{
  "name": "@cogitator-ai/next",
  "version": "0.2.0",
  "description": "Next.js integration for Cogitator AI runtime",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./client": {
      "types": "./dist/client/index.d.ts",
      "import": "./dist/client/index.js"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@cogitator-ai/core": "workspace:*",
    "@cogitator-ai/types": "workspace:*"
  },
  "peerDependencies": {
    "next": ">=14.0.0",
    "react": ">=18.0.0"
  },
  "devDependencies": {
    "@types/react": "^19.2.9",
    "next": "^15.0.0",
    "react": "^19.2.4",
    "typescript": "^5.0.0"
  },
  "keywords": [
    "cogitator",
    "ai",
    "agents",
    "nextjs",
    "streaming",
    "llm"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/cogitator-ai/Cogitator-AI.git",
    "directory": "packages/next"
  }
}
