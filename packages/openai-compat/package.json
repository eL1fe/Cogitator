{
  "name": "@cogitator/openai-compat",
  "version": "0.1.0",
  "description": "OpenAI Assistants API compatibility layer for Cogitator",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "eslint src",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@cogitator/core": "workspace:*",
    "@cogitator/types": "workspace:*",
    "@fastify/cors": "^11.0.1",
    "@fastify/multipart": "^9.3.0",
    "fastify": "^5.2.3",
    "nanoid": "^5.1.5",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/busboy": "^1.5.4",
    "@types/node": "^20.10.0",
    "typescript": "^5.3.0",
    "vitest": "^1.6.0"
  },
  "peerDependencies": {
    "@cogitator/core": "workspace:*",
    "@cogitator/types": "workspace:*"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
