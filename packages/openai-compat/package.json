{
  "name": "@cogitator-ai/openai-compat",
  "version": "10.0.1",
  "description": "OpenAI Assistants API compatibility layer for Cogitator",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "eslint src",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@cogitator-ai/core": "workspace:*",
    "@cogitator-ai/types": "workspace:*",
    "@fastify/cors": "^11.0.1",
    "@fastify/multipart": "^9.3.0",
    "@types/node": "^25.0.0",
    "fastify": "^5.2.3",
    "nanoid": "^5.1.5",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/busboy": "^1.5.4",
    "typescript": "^5.3.0",
    "vitest": "^1.6.0"
  },
  "peerDependencies": {
    "@cogitator-ai/core": "workspace:*",
    "@cogitator-ai/types": "workspace:*"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/eL1fe/cogitator.git",
    "directory": "packages/openai-compat"
  },
  "publishConfig": {
    "access": "public"
  },
  "license": "MIT"
}
