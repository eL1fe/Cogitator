{
  "name": "@cogitator-ai/neuro-symbolic",
  "version": "3.0.0",
  "description": "Neuro-symbolic AI for hybrid neural-symbolic reasoning",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./logic": {
      "types": "./dist/logic/index.d.ts",
      "import": "./dist/logic/index.js"
    },
    "./constraints": {
      "types": "./dist/constraints/index.d.ts",
      "import": "./dist/constraints/index.js"
    },
    "./planning": {
      "types": "./dist/planning/index.d.ts",
      "import": "./dist/planning/index.js"
    },
    "./knowledge-graph": {
      "types": "./dist/knowledge-graph/index.d.ts",
      "import": "./dist/knowledge-graph/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "clean": "rm -rf dist",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "dependencies": {
    "@cogitator-ai/types": "workspace:*",
    "nanoid": "^5.0.4",
    "zod": "^3.22.4"
  },
  "optionalDependencies": {
    "z3-solver": "^4.13.0"
  },
  "devDependencies": {
    "typescript": "^5.3.0",
    "vitest": "^1.0.0"
  },
  "peerDependencies": {
    "@cogitator-ai/core": "workspace:*",
    "@cogitator-ai/memory": "workspace:*"
  },
