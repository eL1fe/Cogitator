  },
  "optionalDependencies": {
    "@extism/extism": "^1.0.3",
    "dockerode": "^4.0.2"
  },
  "devDependencies": {
    "typescript": "^5.3.0",
    "vitest": "^1.0.0"
  },
  "peerDependencies": {
    "@extism/extism": "^1.0.0",
    "dockerode": "^4.0.0"
  },
  "peerDependenciesMeta": {
    "@extism/extism": {
      "optional": true
    },
    "dockerode": {
      "optional": true
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/eL1fe/cogitator.git",
    "directory": "packages/sandbox"
  },
