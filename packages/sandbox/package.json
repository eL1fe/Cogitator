{
  "name": "@cogitator-ai/sandbox",
  "version": "0.2.11",
  "description": "Sandbox execution for Cogitator agents (Docker and WASM)",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "clean": "rm -rf dist",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "dependencies": {
    "@cogitator-ai/types": "workspace:*",
    "@types/dockerode": "^3.3.31",
    "@types/node": "^25.0.0",
