{
  "name": "@cogitator-ai/wasm-tools",
  "version": "0.2.4",
  "description": "WASM-based tools for Cogitator agents",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./wasm/*": "./dist/wasm/*"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc && npm run build:wasm",
    "build:wasm": "node scripts/build-wasm.js",
    "dev": "tsc --watch",
    "clean": "rm -rf dist",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@cogitator-ai/types": "workspace:*",
    "zod": "^3.22.0"
  },
  "devDependencies": {
    "@extism/js-pdk": "^1.0.0",
    "esbuild": "^0.27.2",
    "typescript": "^5.3.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/eL1fe/cogitator.git",
    "directory": "packages/wasm-tools"
  },
  "publishConfig": {
    "access": "public"
  },
  "license": "MIT"
}
