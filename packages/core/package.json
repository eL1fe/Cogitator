{
  "name": "@cogitator-ai/core",
  "version": "0.1.1",
  "description": "Core runtime for Cogitator AI agents",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "clean": "rm -rf dist",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.71.2",
    "@cogitator-ai/memory": "workspace:*",
    "@cogitator-ai/models": "workspace:*",
    "@cogitator-ai/types": "workspace:*",
    "@types/node": "^25.0.0",
    "nanoid": "^5.0.4",
    "openai": "^4.24.0",
    "zod": "^3.22.4",
    "zod-to-json-schema": "^3.22.4"
  },
  "devDependencies": {
    "typescript": "^5.3.0",
    "vitest": "^1.0.0"
  },
  "peerDependencies": {
    "@cogitator-ai/sandbox": "workspace:*"
  },
  "peerDependenciesMeta": {
    "@cogitator-ai/sandbox": {
      "optional": true
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/eL1fe/cogitator.git",
    "directory": "packages/core"
  },
  "publishConfig": {
    "access": "public"
  },
  "license": "MIT"
}
