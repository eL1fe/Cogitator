{
  "name": "cogitator",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "description": "The Sovereign AI Agent Runtime - Self-hosted, production-grade orchestration for LLM swarms and autonomous agents",
  "repository": {
    "type": "git",
    "url": "https://github.com/eL1Fe/cogitator.git"
  },
  "license": "MIT",
  "author": "Your Name",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "cli": "node packages/cli/dist/index.js",
    "build": "turbo run build",
    "dev": "turbo run dev",
    "test": "turbo run test",
    "test:watch": "turbo run test:watch",
    "lint": "eslint 'packages/*/src/**/*.ts'",
    "lint:fix": "eslint 'packages/*/src/**/*.ts' --fix",
    "format": "prettier --write \"**/*.{ts,tsx,md,json}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,md,json}\"",
    "typecheck": "turbo run typecheck",
    "clean": "turbo run clean && rm -rf node_modules",
    "prepare": "node -e \"try { if (process.env.CI !== 'true' && process.env.VERCEL !== '1') require('husky').install() } catch(e) {}\"",
    "changeset": "changeset",
    "version": "changeset version",
    "release": "turbo run build && changeset publish",
    "publish:all": "npx tsx scripts/publish-all.ts",
    "dashboard": "pnpm --filter @cogitator-ai/dashboard dev",
    "dashboard:build": "pnpm --filter @cogitator-ai/dashboard build",
    "dashboard:start": "pnpm --filter @cogitator-ai/dashboard start",
    "services": "docker-compose up -d",
    "services:down": "docker-compose down",
    "start": "pnpm build && concurrently \"pnpm services\" \"pnpm dashboard\""
  },
  "dependencies": {
    "openai": "^6.16.0",
    "typescript": "^5.9.3",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@changesets/cli": "^2.29.8",
    "@eslint/js": "^9.39.2",
    "@types/node": "^25.0.10",
    "@vitest/coverage-v8": "^4.0.18",
    "concurrently": "^9.2.1",
    "eslint": "^9.39.2",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "prettier": "^3.8.1",
    "tsx": "^4.21.0",
    "turbo": "^2.7.5",
    "typescript-eslint": "^8.54.0",
    "vitest": "^4.0.18"
  },
  "packageManager": "pnpm@8.12.0",
  "engines": {
    "node": ">=20.0.0",
    "pnpm": ">=8.0.0"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{md,json}": [
      "prettier --write"
    ]
  }
}
